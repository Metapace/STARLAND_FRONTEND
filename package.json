    "dotenv": "^16.1.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.4.1",
    "husky": "^8.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "turbo": "latest"
  },
  "lint-staged": {
